{
  "manifest_version": 3,
  "name": "Handy Helper",
  "version": "1.3",
  "permissions": [
    "tabs",
    "scripting",
    "storage",
    "downloads"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png"
    }
  },
  "options_page": "options.html"
}